<?xml version="1.0" encoding="UTF-8"?>
<suite name="Nopcommerce" parallel="tests" thread-count="3">
	<listeners>
		<listener class-name="commons.MethodListener"/>
		<listener class-name="reportConfig.ExtentTestListener"/>
		<listener class-name="org.uncommons.reportng.HTMLReporter"/>
		<listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
	</listeners>

	<parameter name="envName" value="local" />
	<parameter name="serverName" value="Test" />
	  
	  	<test name="Run on Firefox">
		<parameter name="browser" value="Chrome" />
		<!-- <parameter name="ipAddress" value="localhost" />-->
		<!-- <parameter name="portNumber" value="4444" />-->
		<parameter name="osName" value="Windows" />
		<parameter name="osVersion" value="10" />
		<classes>
			<class name="com.nopcommerce.HomePage.Home_Page" />
			<class name="com.nopcommerce.LoginPage.Login_Page" />
			<class name="com.nopcommerce.RegisterPage.Register_Page" />
		</classes>
	  </test>
</suite> <!-- Suite -->
